.now-wether {
  color: #fff;
  background-color: #74a5ce;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.now-wether .now {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100vw;
  padding-top: 80rpx;
  background: linear-gradient(to bottom, #649cca, #7badd6);
}
.now-wether .now .tapbar {
  width: 100vw;
  height: 80rpx;
  border-radius: 10rpx;
  background-color: #649cca;
  display: flex;
  z-index: 3;
  position: fixed;
  top: 0;
  justify-content: center;
  align-items: center;
}
.now-wether .now .tapbar .mid .searchicon .van-icon {
  padding-top: 10rpx;
}
.now-wether .now .tapbar .mid .title {
  margin: 10rpx;
}
.now-wether .now .showface {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 100rpx 0;
}
.now-wether .now .showface view {
  padding: 10rpx;
}
.now-wether .now .showface .tempbox {
  display: flex;
  justify-content: center;
  align-items: flex-start;
}
.now-wether .now .showface .tempbox .temp {
  font-size: 250rpx;
  font-weight: lighter;
  padding: 50rpx 0 20rpx 50rpx;
  line-height: 200rpx;
}
.now-wether .now .showface .tempbox .tempc {
  font-size: 50rpx;
  padding: 40rpx 0 0 0;
}
.now-wether .now .nowair {
  width: 100vw;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.now-wether .now .nowair .icon {
  padding: 20rpx;
}
.now-wether .now .nowair text {
  padding: 20rpx 50rpx;
}
.now-wether .future {
  display: flex;
  align-items: center;
  overflow: auto;
  width: 100vw;
  background-color: #89b8df;
  border-radius: 20rpx;
  margin: 20rpx;
}
.now-wether .future .futurebox {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 20vw;
  height: 300rpx;
  border-right: 1px solid #6595bd;
}
.now-wether .future .futurebox view {
  padding: 10rpx 0;
}
.now-wether .future .futurebox .day {
  white-space: nowrap;
}
.now-wether .tip {
  padding: 40rpx;
  box-sizing: border-box;
  width: 100vw;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.now-wether .tip view {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.now-wether .tip view .data {
  font-size: 36rpx;
  color: #fff;
}
.now-wether .tip view text {
  font-size: 24rpx;
  color: #eaeaea;
}
.now-wether .lift-tip {
  width: 100vw;
  display: flex;
  flex-wrap: wrap;
  border-radius: 20rpx;
  background-color: #89b8df;
  margin: 20rpx 0 30rpx 0;
}
.now-wether .lift-tip .icon {
  margin: 5rpx 10rpx 5rpx 0;
}
.now-wether .lift-tip .cell {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  padding: 20rpx;
}
.now-wether .help {
  padding: 20rpx 0;
  font-size: 20rpx;
  color: #89bce6;
}
.now-wether-loading {
  width: 100vw;
  height: 100vh;
  position: relative;
  background: linear-gradient(to bottom, #73a4cc, #7badd6);
  color: #fff;
}
.now-wether-loading .title {
  position: absolute;
  right: 0;
  left: 80rpx;
  top: 230rpx;
  margin: 0 auto ;
  width: 250rpx;
  line-height: 105rpx;
  font-size: 100rpx;
}
