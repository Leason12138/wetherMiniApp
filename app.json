{
    "pages": [
        "pages/home/home",
        "pages/map/map",
        "pages/my/my"
    ],
    "plugins": {
        "chooseLocation": {
            "version": "1.0.5",
            "provider": "wx76a9a06e5b4e693e"
        },
        "routePlan": {
            "version": "1.0.11",
            "provider": "wx50b5593e81dd937a"
        }
    },
    "permission": {
        "scope.userLocation": {
            "desc": "你的位置信息将用于小程序定位"
        }
    },
    "window": {
        "backgroundTextStyle": "light",
        "navigationBarBackgroundColor": "#fff",
        "navigationBarTitleText": "查悦天气",
        "navigationBarTextStyle": "black"
    },
    "tabBar": {
        "color": "#7A7E83",
        "selectedColor": "#649cca",
        "borderStyle": "white",
        "backgroundColor": "#ffffff",
        "list": [{
                "pagePath": "pages/home/home",
                "iconPath": "./image/weather.png",
                "selectedIconPath": "./image/weather.png",
                "text": "天气"
            },
            {
                "pagePath": "pages/map/map",
                "iconPath": "./image/map.png",
                "selectedIconPath": "./image/map.png",

                "text": "地图"
            },
            {
                "pagePath": "pages/my/my",
                "iconPath": "./image/note.png",
                "selectedIconPath": "./image/note.png",

                "text": "我的"
            }
        ]
    },
    "sitemapLocation": "sitemap.json",
    "usingComponents": {
        "van-nav-bar": "@vant/weapp/nav-bar/index",
        "van-icon": "@vant/weapp/icon/index",
        "van-field": "@vant/weapp/field/index",
        "van-toast": "@vant/weapp/toast/index",
        "van-button": "@vant/weapp/button/index",
        "van-search": "@vant/weapp/search/index",
        "van-popup": "@vant/weapp/popup/index",
        "van-cell": "@vant/weapp/cell/index",
        "van-cell-group": "@vant/weapp/cell-group/index"
    }
}